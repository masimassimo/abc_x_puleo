<?xml version="1.0" encoding="utf-8"?>
<!-- Creo un nuovo formato carta per il Preventivo/Ordine di vendita personalizzato per Puleo -->
<odoo>
    <record id="formato_carta_barcode" model="report.paperformat">
        <field name="name">Formato carta Barcode</field>
        <field name="default" eval="False" />
        <field name="format">custom</field>
        <field name="page_height">61</field>
        <field name="page_width">50</field>
        <field name="orientation">Landscape</field>
        <field name="margin_top">1</field>
        <field name="margin_bottom">1</field>
        <field name="margin_left">1</field>
        <field name="margin_right">3.5</field>
        <field name="header_line" eval="False" />
        <field name="header_spacing">0</field>
        <field name="dpi">90</field>
    </record>
</odoo>
